{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "installCommand": "npm ci",
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/$1"
      },
      {
        "source": "/reset-password/(.*)",
        "destination": "/reset-password.html"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }